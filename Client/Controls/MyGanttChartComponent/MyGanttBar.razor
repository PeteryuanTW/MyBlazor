<button type="button" class="btn btn-primary" data-toggle="tooltips" title="@toolTipsString" style="margin-left:@startPositionPercent%; width:@widthPercent%">
	@wo
</button>





@code {
	[Parameter]
	public string wo { get; set; } = "";
	[Parameter]
	public int startTime { get; set; } = 0;
	[Parameter]
	public int endTime { get; set; } = 0;
	[Parameter]
	public float startPositionPercent{ get; set; } = 0.0f;
	[Parameter]
	public float widthPercent{ get; set; } = 0.0f;
	public String toolTipsString { get; set; } = "";

	protected override void OnParametersSet()
	{
		base.OnParametersSet();
		toolTipsString = "start: " + startTime.ToString() + "\n" + "end: " + endTime.ToString();
	}
}
