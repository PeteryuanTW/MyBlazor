@using MyBlazor.Shared.DataClass

    <li>@node.name
    @if (node.children.Any())
    {
        <ul id="@Level">
        @foreach(var child in node.children)
        {
            <TreeNode node=@child Level="@(Level + 1)" @key=child/>
        }
        </ul>
    }
</li>



@code {
  [Parameter]
  public MyTreeNode? node { get; set; }
  [Parameter]
  public int Level { get; set; }
}